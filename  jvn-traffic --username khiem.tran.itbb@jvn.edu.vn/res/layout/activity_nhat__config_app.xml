<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/tittle_bar" >

        <Button
            android:id="@+id/buttonBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_back" />
    </LinearLayout>

    
    
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout2"
        android:orientation="vertical"
        android:overScrollMode="ifContentScrolls" >

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nâng Cao"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center" >
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_toLeftOf="@+id/btnLuuLai"
        android:text="Đổi pin"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:text="Đổi mật khẩu"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignRight="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:text="Đổi users"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:text="Quản lý thiết bị"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    
     
     
     <ToggleButton
         android:id="@+id/toggleButton1"
         android:layout_width="60dp"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView5"
         android:layout_toRightOf="@+id/textView3"
         android:text="ToggleButton" />

   
     
      <ToggleButton
         android:id="@+id/toggleButton2"
         android:layout_width="60dp"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/toggleButton1"
         android:layout_below="@+id/toggleButton1"
         android:layout_toRightOf="@+id/textView3"
         android:text="ToggleButton" />

  
    
    
     
      <ToggleButton
         android:id="@+id/toggleButton3"
         android:layout_width="60dp"
         android:layout_height="wrap_content"
         android:layout_below="@+id/toggleButton2"
        android:layout_toRightOf="@+id/textView3"
         android:text="ToggleButton" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView4"
        android:text="Thường"
        android:textAppearance="?android:attr/textAppearanceMedium" />

      <ToggleButton
          android:id="@+id/toggleButton4"
          android:layout_width="60dp"
          android:layout_height="wrap_content"
          android:layout_below="@+id/toggleButton3"
          android:layout_toRightOf="@+id/textView3"
          android:text="ToggleButton" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignTop="@+id/toggleButton4"
        android:text="Tự khóa"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignTop="@+id/toggleButton1"
        android:text="Call"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignTop="@+id/toggleButton2"
        android:text="Còi"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/toggleButton2"
        android:text="SMS"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView11"
        android:layout_below="@+id/toggleButton4"
        android:text="Signal"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    
     
       <ToggleButton
         android:id="@+id/toggleButton5"
         android:layout_width="60dp"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/toggleButton4"
         android:layout_alignTop="@+id/textView7"
         android:text="ToggleButton" />

     <TextView
         android:id="@+id/textView10"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/textView7"
         android:layout_below="@+id/toggleButton5"
         android:text="Âm lượng"
         android:textAppearance="?android:attr/textAppearanceMedium" />

     <SeekBar
         android:id="@+id/seekBar4"
         android:layout_width="60dp"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/toggleButton5"
         android:layout_alignTop="@+id/textView10"
         android:ems="10" />

     <Button
         android:id="@+id/btnLuuLai"
         android:layout_width="wrap_content"
         android:layout_height="40dp"
         android:layout_alignParentBottom="true"
         android:layout_alignParentRight="true"
         android:layout_marginRight="91dp"
         android:gravity="center"
         android:text="Lưu Lại" />

     <Button
         android:id="@+id/btnBoQua"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentBottom="true"
         android:layout_alignTop="@+id/btnLuuLai"
         android:layout_toLeftOf="@+id/textView10"
         android:text="Bỏ Qua" />

</RelativeLayout>